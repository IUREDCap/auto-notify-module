{
	"name" : "iu-redcap/auto-notify-module",
	"license" : "BSD 3-Clause",
	"description" : "Auto-Notify Module",
	"keywords" : [
		"REDCap", "Auto-Notify"
	],
    "config": {
        "vendor-dir": "dev"
    },
    "require": {
        "ezyang/htmlpurifier": "4.17.0"
    },
    "require-dev": {
        "phpunit/phpunit": "11.0.6",
        "squizlabs/php_codesniffer": "3.9.0"
    },
	"autoload" : {
		"psr-4" : {
			"IU\\AutoNotifyModule\\" : ["./", "./classes/"]
		}
	},
    "autoload-dev": {
        "psr-4" : {
            "IU\\AutoNotifyModule\\": [
                "tests/unit"
            ],
            "ExternalModules\\": [
                "tests/external"
            ]
        }
    }
}
